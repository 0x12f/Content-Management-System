            "homepage": "https://alksily.getwebspace.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-yaml": "*",
        "ext-soap": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "slim/slim": "^4.11.0",
        "slim/psr7": "^1.6",
        "slim/twig-view": "^3.3.0",
        "slim/http-cache": "^1.1.0",
        "php-di/php-di": "^6.4.0",
        "doctrine/orm": "^2.14.1",
        "firebase/php-jwt": "^6.4.0",
        "alksily/support": "^2.0.0",
        "twig/twig": "^v3.5.1",
        "twig/intl-extra": "^v3.5.1",
        "twig/string-extra": "^v3.5.1",
        "ramsey/uuid": "^4.7.3",
        "ramsey/uuid-doctrine": "^1.8.2",
        "phpmailer/phpmailer": "^v6.7.1",
        "sendpulse/rest-api": "^1.0.26",
        "pragmarx/google2fa": "^v4.0.2",
        "phpoffice/phpspreadsheet": "^1.27.1",
        "bacon/bacon-qr-code": "^2.0.8",
        "monolog/monolog": "^2.9.1",
        "illuminate/collections": "^v9.52.0",
        "symfony/cache": "^v6.2.5",
        "symfony/var-dumper": "^v6.2.5",
        "voku/html-compress-twig": "^4.0.2",
        "rybakit/twig-deferred-extension": "^v3.0.4",
        "socialconnect/http-client": "^1.0.0",
        "socialconnect/auth": "^3.3.4",
        "bhaktaraz/php-rss-generator": "dev-master"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.21.0",
        "friendsofphp/php-cs-fixer": "^v3.14.4",
        "phpunit/phpunit": "^9.6.3",
        "guzzlehttp/guzzle": "^7.5.0"
    },
    "autoload": {
        "psr-4": {
            "Plugin\\": "plugin/",
            "App\\": "src/"
        },
        "files": [
            "config/vars.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }

        "slim/slim": "^4.12.0",
        "slim/psr7": "^1.6.1",
        "slim/twig-view": "^3.3.0",
        "slim/http-cache": "^1.1.0",
        "php-di/php-di": "^6.4.0",
        "doctrine/orm": "^2.14.2",
        "doctrine/migrations": "^3.6",
        "illuminate/events": "^v10.46.0",
        "illuminate/database": "^v10.46.0",
        "illuminate/collections": "^v10.46.0",
        "firebase/php-jwt": "^6.4.0",
        "alksily/support": "^2.0.0",
        "twig/twig": "^v3.5.1",
        "twig/intl-extra": "^v3.5.1",
        "twig/string-extra": "^v3.5.1",
        "ramsey/uuid": "^4.7.4",
        "ramsey/uuid-doctrine": "^1.8.2",

    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-yaml": "*",
        "ext-soap": "*",
        "ext-curl": "*",
        "slim/slim": "^4.10.0",
        "slim/psr7": "^1.5",
        "slim/twig-view": "^3.3.0",
        "slim/http-cache": "^1.1.0",
        "php-di/php-di": "^6.4.0",
        "doctrine/orm": "^2.12.2",
        "doctrine/migrations": "^3.5.1",
        "alksily/support": "^1.3.1",
        "twig/twig": "^v3.4.3",
        "twig/intl-extra": "^v3.4.0",
        "twig/string-extra": "^v3.4.0",
        "ramsey/uuid": "^4.3.1",
        "ramsey/uuid-doctrine": "^1.8.1",
        "phpmailer/phpmailer": "^v6.6.0",
        "sendpulse/rest-api": "^1.0.26",
        "pragmarx/google2fa": "^v4.0.2",
        "phpoffice/phpspreadsheet": "^1.23.0",
        "bacon/bacon-qr-code": "^2.0.7",
        "monolog/monolog": "^2.6.0",
        "illuminate/collections": "^v9.45.1",
        "symfony/cache": "^v6.1.0",
        "symfony/var-dumper": "^v6.1.0",
        "voku/html-compress-twig": "^4.0.2",
        "rybakit/twig-deferred-extension": "^v3.0.1",
        "bhaktaraz/php-rss-generator": "dev-master",
        "socialconnect/http-client": "^1.0",
        "socialconnect/auth": "^3.0.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Plugin\\": "plugin/",
            "App\\": "src/"
        },
        "files": [
            "config/vars.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}

{
    "name": "getwebspace/platform",
    "description": "Content Management System",
    "authors": [
        {
            "name": "Aleksey Ilyin",
            "email": "alksily@getwebspace.org",
            "homepage": "https://alksily.getwebspace.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "slim/slim": "^4.13.0",
        "slim/psr7": "^1.6.1",
        "slim/twig-view": "^3.3.0",
        "slim/http-cache": "^1.1.0",
        "php-di/php-di": "^7.0.6",
        "illuminate/events": "^v10.48.3",
        "illuminate/database": "^v10.48.3",
        "illuminate/collections": "^v10.48.3",
        "firebase/php-jwt": "^v6.10.0",
        "alksily/support": "^2.0.0",
        "twig/twig": "^v3.8.0",
        "twig/intl-extra": "^v3.8.0",
        "twig/string-extra": "^v3.8.0",
        "ramsey/uuid": "^4.7.2",
        "phpmailer/phpmailer": "^v6.9.1",
        "sendpulse/rest-api": "^2.0.0",
        "phpoffice/phpspreadsheet": "^1.29.0",
        "bacon/bacon-qr-code": "^2.0.8",
        "monolog/monolog": "^3.5.0",
        "symfony/cache": "^v7.0.0",
        "symfony/var-dumper": "^v7.0.0",
        "voku/html-compress-twig": "^4.0.2",
        "rybakit/twig-deferred-extension": "^v3.0.4",
        "robmorgan/phinx": "^0.16.0"
    },
    "require-dev": {
        "fakerphp/faker": "^2.0.x-dev",
        "friendsofphp/php-cs-fixer": "^v3.17.0",
        "phpunit/phpunit": "^9.6.9",
        "guzzlehttp/guzzle": "^7.7.0"
    },
    "autoload": {
        "psr-4": {
            "Plugin\\": "plugin/",
            "App\\": "src/"
        },
        "files": [
            "config/vars.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}

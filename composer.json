{
    "name": "getwebspace/platform",
    "description": "Content Management System structure",
    "authors": [
        {
            "name": "Aleksey Ilyin",
            "email": "alksily@icloud.com",
            "homepage": "https://www.facebook.com/aleksey.ily",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-yaml": "*",
        "slim/slim": "^3.12.3",
        "slim/php-view": "^2.2.1",
        "slim/twig-view": "^2.5.1",
        "slim/http-cache": "^0.4.0",
        "doctrine/orm": "^2.8.1",
        "doctrine/migrations": "^3.1",
        "alksily/support": "^1.2.1",
        "twig/twig": "2.12.1",
        "twig/extensions": "^1.5.4",
        "twig/intl-extra": "3.x-dev",
        "phive/twig-extensions-deferred": "^2.0.3",
        "ramsey/uuid": "^3.9.3",
        "ramsey/uuid-doctrine": "^1.6",
        "bhaktaraz/php-rss-generator": "dev-master",
        "phpmailer/phpmailer": "^v6.4.1",
        "pragmarx/google2fa": "^3.0.3",
        "phpoffice/phpspreadsheet": "^1.15.0",
        "bacon/bacon-qr-code": "^1.0.3",
        "monolog/monolog": "^1.25.5",
        "runcmf/runtracy": "^0.2.10",
        "nochso/html-compress-twig": "^2.0.1",
        "illuminate/collections": "^8.17.2"
    },
    "require-dev": {
        "fzaninotto/faker": "@stable",
        "friendsofphp/php-cs-fixer": "@stable",
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Plugin\\": "plugin/",
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
